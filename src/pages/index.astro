---
import MainLayout from "../layouts/MainLayout.astro";
import Button from "../components/Button.astro";
import Link from "../components/Link.astro";
import { Image } from "astro:assets";
import thumbsup from "../assets/images/thumbsup.png";
import Testemonial from "../components/Testemonial.astro";
---

<MainLayout title="Titel">
  <article>
    <h1>Index overskrift</h1>

    <p>
      Link til <Link href="https://www.kea.dk" target="_blank"
        >En anden side</Link
      >
    </p>
    <Image src={thumbsup} alt="" />
  </article>
  <section>
    <h2>Testemonial</h2>
    <Testemonial titel="Karen Wolff" stjerner="6" anmeldelseid="anmeldelse1">
      “Vil du opleve Tothavens dyr og stemning helt tæt på? Køb din billet her
      og glæd dig til en dag fyldt med nærvær, leg og læring i naturskønne
      omgivelser.”
    </Testemonial>
  </section>
</MainLayout>
